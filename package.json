{
  "name": "@leonardodino/gatsby-plugin-amp",
  "version": "1.0.0-alpha.8",
  "description": "A gatsby plugin for scaffolding AMP pages",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__",
    "commit": "git add . && git commit && git push -u origin master"
  },
  "author": "Leonardo Dino",
  "license": "MIT",
  "homepage": "https://github.com/leonardodino/gatsby-plugin-amp#readme",
  "repository": "https://github.com/leonardodino/gatsby-plugin-amp",
  "bugs": {
    "url": "https://github.com/leonardodino/gatsby-plugin-amp/issues"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.4",
    "clean-css": "^4.2.1",
    "got": "^9.6.0",
    "image-size": "^0.7.4",
    "jsdom": "^13.0.0",
    "lodash.flattendeep": "^4.4.0",
    "react": "^16.6.1",
    "sync-rpc": "^1.3.4"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-plugin-amp",
    "amp",
    "accelerated-mobile-pages"
  ],
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "~7.3.0",
    "babel-preset-gatsby-package": "^0.1.4",
    "then-request": "^6.0.2"
  },
  "resolutions": {
    "babel-preset-gatsby-package/@babel/preset-env": "~7.3.0"
  }
}
